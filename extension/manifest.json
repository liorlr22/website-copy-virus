{
    "name": "calculator",
    "description": "Simple calculation is now possible!",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["background.js"]
    }],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Js calculator"
      },     
     "permissions": ["tabs"]
}